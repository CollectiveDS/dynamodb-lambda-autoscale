{
  "ReadCapacity": {
    "Min": 10,
    "Max": 3000,
    "Increment": {
      "When": {
        "UtilisationIsAbovePercent": 80,
        "ThrottledEventsPerMinuteIsAbove": 2
      },
      "To": {
        "ConsumedPercent": 200
      }
    },
    "Decrement": {
      "When": {
        "UtilisationIsBelowPercent": 50,
        "AfterLastIncrementMinutes": 60,
        "AfterLastDecrementMinutes": 60,
        "UnitAdjustmentGreaterThan": 5
      },
      "To": {
        "ConsumedPercent": 120
      }
    }
  },
  "WriteCapacity": {
    "Min": 10,
    "Max": 3000,
    "Increment": {
      "When": {
        "UtilisationIsAbovePercent": 80,
        "ThrottledEventsPerMinuteIsAbove": 2
      },
      "To": {
        "ConsumedPercent": 200
      }
    },
    "Decrement": {
      "When": {
        "UtilisationIsBelowPercent": 50,
        "AfterLastIncrementMinutes": 60,
        "AfterLastDecrementMinutes": 60,
        "UnitAdjustmentGreaterThan": 5
      },
      "To": {
        "ConsumedPercent": 120
      }
    }
  }
}
